<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="SBP2.Views.KlaseUserControls.BoracUserControl">
    <StackPanel HorizontalAlignment="Center" VerticalAlignment="Top">
        <TextBlock HorizontalAlignment="Center" FontSize="24" Margin="0 0 0 20">Borac</TextBlock>
        <Label HorizontalAlignment="Left" VerticalAlignment="Center" Margin="0 0 0 10">DualWielder:</Label>
        <CheckBox IsCheckedChanged="DualWielderCbx_OnIsCheckedChanged" x:Name="DualWielderCbx" Width="200" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0 0 0 10"/>
        <Label HorizontalAlignment="Left" VerticalAlignment="Center" Margin="0 0 0 10">Koristi stit:</Label>
        <CheckBox IsCheckedChanged="KoristiStitCbx_OnIsCheckedChanged" x:Name="KoristiStitCbx" Width="200" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0 0 0 10"/>
    </StackPanel>
</UserControl>
